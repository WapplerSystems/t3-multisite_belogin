{
	"name": "wapplersystems/multisite-belogin",
	"type": "typo3-cms-extension",
	"description": "Cross site/domain backend login for TYPO3. It allows backend users to work in the frontend across domains.",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"TYPO3",
		"extension",
		"multisite",
		"backend",
		"login",
		"cross domain"
	],
	"homepage": "https://wappler.systems",
	"require": {
		"typo3/cms-core": "^11.5.33"
	},
	"autoload": {
		"psr-4": {
			"WapplerSystems\\MultisiteBelogin\\": "Classes"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "multisite_belogin"
		}
	}
}
